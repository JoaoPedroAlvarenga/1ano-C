package com.projeto.operadores;

public class Texto {
	public static void main(String[] args){
		System.out.println(Character.isDigit('c')); // testas se é numero
		System.out.println(Character.isDigit('5'));
		
		System.out.println(Character.isLetter('c')); //testa se é letra
		System.out.println(Character.isLetter('5'));
		
		System.out.println(Character.isLetterOrDigit('c')); // testa se é uma das anteriores
		System.out.println(Character.isLetterOrDigit('5'));
		
		System.out.println(Character.isUpperCase('c')); // se está em maiúsculo
		System.out.println(Character.isUpperCase('C'));
		
		System.out.println(Character.isLowerCase('c')); // minúsculo
		System.out.println(Character.isLowerCase('C'));

		System.out.println(Character.isWhitespace(' ')); // se está void
		
			String arquivo = "prova.html";
		    arquivo = arquivo.replace("html", "jps");
		    System.out.println(arquivo);
		    
		
	}

}
